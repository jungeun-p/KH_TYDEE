<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>Tydee_tip</display-name>
  <welcome-file-list>
    <welcome-file>tip_index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>tip</servlet-name>
  	<servlet-class>com.tydee.tip.controller.tip_controller</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>tip</servlet-name>
  	<url-pattern>/tip.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>userinfo</servlet-name>
  	<servlet-class>com.tydee.tip.controller.UserInfoController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>userinfo</servlet-name>
  	<url-pattern>/member.do</url-pattern>
  </servlet-mapping>
  
  
</web-app>